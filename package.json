{
  "name": "library-assistant-web",
  "private": true,
  "dependencies": {
    "@rails/webpacker": "^3.4.1",
    "axios": "^0.18.0",
    "vue": "^2.5.16",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.13",
    "babel-jest": "^22.4.3",
    "jest": "^22.4.3",
    "vue-jest": "^2.3.0",
    "webpack-dev-server": "2.11.2"
  },
  "scripts": {
